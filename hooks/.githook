#!/bin/sh
pre_commit_modules="prevent-master-commit.sh prevent-merge-marker-commits.sh prevent-large-commits.sh prevent-large-files.sh"
prevent_master_commit=ENABLED
prevent_large_files=ENABLED
prevent_large_commits=ENABLED

hard_filesize_limit=65536
soft_filesize_limit=32768

soft_filecount_limit=8
hard_filecount_limit=16

REPO_PATH=$(git rev-parse --show-toplevel)
